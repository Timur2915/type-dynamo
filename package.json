{
  "name": "type-dynamo",
  "version": "0.1.3",
  "description": "Easy-to-use ORM for your Typescript projects with DynamoDB and Node.js",
  "bin": {
    "type-dynamo": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "docker": "docker-compose up",
    "playground": "ts-node playground/index.ts",
    "test": "jest --collectCoverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "test:acceptance": "cucumber-js --require 'acceptance/support/**/*.ts' --require-module ts-node/register acceptance"
  },
  "jest": {
    "testRegex": "(spec/.*|(\\.|/)(test|spec))\\.ts$",
    "transform": {
      ".(ts)": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "author": "lucasmafra",
  "license": "ISC",
  "dependencies": {
    "amazon-dax-client": "^1.0.2",
    "aws-sdk": "git+https://git@github.com/lucasmafra/aws-sdk-js.git"
  },
  "devDependencies": {
    "@types/cucumber": "^4.0.4",
    "@types/jest": "^23.3.2",
    "@types/node": "^8.0.47",
    "coveralls": "^3.0.2",
    "cucumber": "^5.0.1",
    "jest": "^23.6.0",
    "jest-plugin-context": "^2.9.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^5.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.6.1"
  }
}
