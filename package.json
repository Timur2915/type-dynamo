{
  "name": "type-dynamo",
  "version": "0.1.3",
  "description": "Easy-to-use ORM for your Typescript projects with DynamoDB and Node.js",
  "bin": {
    "type-dynamo": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "docker": "docker-compose up",
    "playground": "ts-node playground/index.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testRegex": "(spec/.*|(\\.|/)(test|spec))\\.ts$",
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "author": "lucasmafra",
  "license": "ISC",
  "dependencies": {
    "amazon-dax-client": "^1.0.2",
    "aws-sdk": "git+https://git@github.com/lucasmafra/aws-sdk-js.git",
    "commander": "^2.11.0",
    "dynamodb-marshaler": "^2.0.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.105",
    "@types/node": "^8.0.47",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^5.0.0",
    "typescript": "^2.6.1"
  }
}
